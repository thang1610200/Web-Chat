	<script src="https://cdn.socket.io/4.5.0/socket.io.min.js" integrity="sha384-7EyYLQZgWBi67fBtVxw60/OWl1kjsfrPFcaU0pp0nAh+i8FD068QogUvg85Ewy1k" crossorigin="anonymous"></script>
	<script>
		var socket = io.connect("http://localhost:3000");
		socket.on("connect",function(){
			console.log("Connect");
		});

		//bắt sự kiện update-message từ server
		socket.on("update-message",function(data){
			var nodelist = document.querySelectorAll('.conversations');
			if(data.sender == "me"){
			nodelist[data.stt - 1].innerHTML += "<li class='me'> <figure><img width='45px' height='45px' src='/static/images/user/<%= data.image%>' alt=''></figure> <div class='text-box'><p>"+data.message+"</p><span><i class='ti-check'></i><i class='ti-check'></i> <%= new Date().toLocaleTimeString().slice(0,4) + new Date().toLocaleTimeString().slice(8,10)%></span></div></li>";
				
			}

			if(data.sender == "you"){
			nodelist[data.stt - 1].innerHTML += "<li class='you'> <figure><img width='45px' height='45px' src='/static/images/user/"+data.image+"' alt=''></figure> <div class='text-box'><p>"+data.message+"</p><span><i class='ti-check'></i><i class='ti-check'></i> <%= new Date().toLocaleTimeString().slice(0,4) + new Date().toLocaleTimeString().slice(8,10)%></span></div></li>";
			}
		});

		//tạo sự kiện gửi tin nhắn ở phía client
		$(".btn_send").click(function(){
			var message = $(this).prev(".messagee").val();
			var stt = $(this).attr("saveid");
			var image = $(this).attr("saveimage");
			$(this).prev(".messagee").val("");
			if(message.trim().length != 0){
				socket.emit("send-message",message,stt,image);  // khởi tạo sự kiện send-message
				var data = {
					sender: $(this).attr("sender"),
					receiver: $(this).attr("savefriend"),
					message: message
				}
				var base_url = location.protocol + "//" + document.domain + ":" + location.port;
				$.ajax({
					url: base_url + "/chat/addchat",
				type: "POST",
				data:data,
				dataType: "JSON",
				success: function(res){
					if(res && res.status_code == 200){
						console.log("OK");
					}
				}
				});
			}
		});
		$("form.chatmessage").submit(function(){
			return false;
		});
		
		$(".messagee").keypress(function(e){
			if(e.which == 13){
				$(".btn_send").trigger("click");
			}
		});

		//video - call
		$('.audio-call, .video-call').on('click', function () {
		var id = $(this).attr("id_video_call");
		var sender = $(this).attr("sender_video");
		var save = "#call-wraper" + id;
		let room = sender + id;
		let acceptsender = "#accept-call"+id;
		let url = "/chat/video/" +room;
		$(acceptsender).attr("href",url);
		$(save).addClass('active');
		socket.emit("send-video",id,sender);
		});
		$('.decline-call, .later-rmnd').on('click', function () {
		$('.call-wraper').removeClass('active');
		});

		socket.on("receive-video",function(data){
			var save = "#call-wraper" + data.sender;
			if($('#saveinforuser').val() == data.id){
				let room = data.sender + data.id;
				$(save).addClass('active');
				let acceptid = "#accept-call"+data.sender;
				let url = "/chat/video/" +room;
				$(acceptid).attr("href",url);
			}
		});

		$('.accept-call').click(function(){
			var id = $(this).attr("id");
			var save = "#call-wraper" + id.slice(12);
			//console.log(id.slice(12));
			$(save).removeClass('active');
		});
		
</script>
